{
  "name": "baldeweg/extra-bundle",
  "description": "Offers tools for management of users, testing and setup.",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "AndrÃ© Baldeweg",
      "email": "a@baldeweg.org"
    }
  ],
  "require": {
    "php": ">=8.0",
    "doctrine/orm": "2.8.4",
    "symfony/config": "5.2.4",
    "symfony/console": "5.2.6",
    "symfony/dependency-injection": "5.2.6",
    "symfony/form": "5.2.6",
    "symfony/framework-bundle": "5.2.6",
    "symfony/http-foundation": "5.2.4",
    "symfony/http-kernel": "5.2.6",
    "symfony/maker-bundle": "1.30.2",
    "symfony/options-resolver": "5.2.4",
    "symfony/security-bundle": "5.2.6"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "2.18.6",
    "phpunit/phpunit": "9.5.4",
    "rector/rector": "0.10.11",
    "symfony/browser-kit": "5.2.4",
    "symfony/var-dumper": "5.2.6"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Baldeweg\\Bundle\\ExtraBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Baldeweg\\Bundle\\ExtraBundle\\Tests\\": "tests/"
    }
  }
}
